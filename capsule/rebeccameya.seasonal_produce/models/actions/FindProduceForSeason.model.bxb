action (FindProduceForSeason) {
  description (Searches for the produce available in the requested season)
  type(Search)
  collect {
    input (season) {
      type (Season)
      min (Required) max (One)
    }
    input (produceType) {
      type (ProduceType)
      min (Optional) max (One)
    }
  }
  output (Produce)
}